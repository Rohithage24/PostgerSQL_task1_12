generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  Id Int @id @default(autoincrement())
  Name String? 
  Mobile String @unique
  created_date DateTime @default(now())
}

model MobileLogin {
  Id         Int      @id @default(autoincrement())
  Mobile     String   @unique
  Name       String
  userName   String   @unique
  password   String
  otp        String?
}